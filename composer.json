	"description": "Email User Agent",
	"license": "MIT",
	"autoload": {
		"psr-4": {
			"App\\": "./lib",
			"Custom\\": "./Custom"
		}
	},
	"repositories": [
		{
			"type": "vcs",
			"url": "https://github.com/pear/Auth_SASL"
		}
	],
	"require": {
		"php": ">=7.3.0",
		"ext-curl": "*",
		"ext-date": "*",
		"ext-imap": "*",
		"ext-json": "*",
		"ext-PDO": "*",
		"ext-session": "*",
		"ext-sockets": "*",
		"edoceo/lowline_php": "dev-master",
		"edoceo/radix": "dev-master",
		"google/apiclient": "*",
		"league/oauth2-client": "2.4.1",
		"pear/auth_sasl": "*",
		"pear/net_smtp": "1.8.1",
		"pear/net_imap": "1.1.3",
		"php-mime-mail-parser/php-mime-mail-parser": "^6.0"
